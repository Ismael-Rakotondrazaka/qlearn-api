    "vue": "latest",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0",
    "xss": "^1.0.15",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^20.14.10",
    "eslint": "^8.57.0",
